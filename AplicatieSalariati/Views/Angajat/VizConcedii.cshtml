@model IEnumerable<AplicatieSalariati.Models.ConcediiModel>
@{
    ViewBag.Title = ViewBag.Type;
}
<h2>Concedii</h2>

<div class="col-md-12">
    <div class="table-responsive">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.dataStart)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DataFinal)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Confirmat)
                </th>
                <th></th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.dataStart)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DataFinal)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Confirmat)
                    </td>
                    @*<td class="text-right">
                        <a type="button" href="@Url.Action("Edit", "Angajat", new { id = item.CNP})" class="btn btn-primary" aria-label="Left Align">
                            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
                        </a>
                    </td>*@
                </tr>
            }
        </table>
    </div>
</div>